<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="0.0.1"/>
		<!-- name.en(dug)="Duruma" -->
		<language type="dug"/>
		<special>
			<sil:identity defaultRegion="KE" script="Latn"/>
		</special>
	</identity>
	<localeDisplayNames>
		<languages>
			<language type="dug">Chiduruma</language>
		</languages>
		<special>
			<sil:names>
				<sil:name xml:lang="en">Duruma</sil:name>
			</sil:names>
		</special>
	</localeDisplayNames>
	<characters>
		<exemplarCharacters>[a á b c d e f g h i j k l m n o p r s t u v w y z ʼ]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[q x]</exemplarCharacters>
		<exemplarCharacters type="index" draft="generated">[A Á B C D E F G H I J K L M N O P R S T U V W Y Z ʼ]</exemplarCharacters>
		<exemplarCharacters type="punctuation" draft="generated">[! ( ) , \- . \: ; ? ‘ ’ “ ”]</exemplarCharacters>
	</characters>
	<delimiters>
		<quotationStart>“</quotationStart>
		<quotationEnd>”</quotationEnd>
		<alternateQuotationStart>‘</alternateQuotationStart>
		<alternateQuotationEnd>’</alternateQuotationEnd>
		<special>
			<sil:matched-pairs>
				<sil:matched-pair open="(" close=")"/>
				<sil:matched-pair open="[" close="]"/>
				<sil:matched-pair open="{" close="}"/>
			</sil:matched-pairs>
		</special>
	</delimiters>
	<collations>
		<collation type="standard">
			<cr><![CDATA[
				&C << ch <<< Ch <<< CH << chʼ <<< Chʼ <<< CHʼ
				&D << dz <<< Dz <<< DZ
				&H << hʼ <<< Hʼ
				&K << kʼ <<< Kʼ << kwʼ <<< Kwʼ <<< KWʼ
				&N << ngʼ <<< Ngʼ <<< NGʼ
				&P << pʼ <<< Pʼ << ph <<< Ph <<< PH << pwʼ <<< Pwʼ <<< PWʼ
				&S << sh <<< Sh <<< SH
				&T << tʼ <<< Tʼ << tsʼ <<< Tsʼ <<< TSʼ << twʼ <<< Twʼ <<< TWʼ << tswʼ <<< Tswʼ <<< TSwʼ <<< TSWʼ
				&Z << zh <<< Zh <<< ZH
			]]></cr>
			<special>
				<sil:simple>a/A á/Á
				b/B
				c/C ch chʼ
				d/D dz
				e/E
				f/F
				g/G
				h/H hʼ
				i/I
				j/J
				k/K kʼ kwʼ
				l/L
				m/M
				n/N ngʼ
				o/O
				p/P pʼ ph pwʼ
				r/R
				s/S sh
				t/T tʼ tsʼ twʼ tswʼ
				u/U
				v/V
				w/W
				y/Y
				z/Z zh</sil:simple>
			</special>
		</collation>
	</collations>
	<special>
		<sil:external-resources>
			<sil:font name="Charis SIL" types="default">
				<sil:url>https://lff.api.languagetechnology.org/family/charissil</sil:url>
			</sil:font>
			<sil:font name="Noto Sans">
				<!--types="ui"-->
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSans/NotoSans-Regular.ttf</sil:url>
			</sil:font>
			<sil:font name="Noto Serif">
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSerif/NotoSerif-Regular.ttf</sil:url>
			</sil:font>
		</sil:external-resources>
	</special>
</ldml>
