<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="0.0.1"/>
		<!-- name.en(enq)="Enga" -->
		<language type="enq"/>
		<special>
			<sil:identity defaultRegion="PG" script="Latn"/>
		</special>
	</identity>
	<localeDisplayNames>
		<languages>
			<language type="enq">Enga</language>
		</languages>
		<special>
			<sil:names>
				<sil:name xml:lang="en">Enga</sil:name>
			</sil:names>
		</special>
	</localeDisplayNames>
	<characters>
		<exemplarCharacters>[a b d e g i j k l {ly} m {mb} n {nde} {ngi} {nj} {ny} ŋ o p s t u w y]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[c f h q r v x z]</exemplarCharacters>
		<exemplarCharacters type="index">[A B D E G I J K L M N Ŋ O P R S T U V W Y]</exemplarCharacters>
		<exemplarCharacters type="punctuation">[\- – , ; \: ! ? . ‘ ’ “ ” ( )]</exemplarCharacters>
	</characters>
	<delimiters>
		<special>
			<sil:matched-pairs>
				<sil:matched-pair open="(" close=")"/>
				<sil:matched-pair open="[" close="]"/>
				<sil:matched-pair open="{" close="}"/>
			</sil:matched-pairs>
		</special>
	</delimiters>
	<special>
		<sil:external-resources>
			<sil:font name="Charis" types="default" features="cv43=2">
				<sil:url>https://lff.api.languagetechnology.org/family/charis</sil:url>
			</sil:font>
			<sil:font name="Noto Sans">
				<!--types="ui"-->
