<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="0.0.1"/>
		<!-- name.en(ajg)="Aja (Benin)" -->
		<language type="ajg"/>
		<special>
			<sil:identity defaultRegion="BJ" script="Latn"/>
		</special>
	</identity>
	<localeDisplayNames>
		<languages>
			<language type="ajg">Ajagbe</language>
		</languages>
		<special>
			<sil:names>
				<sil:name xml:lang="en">Aja</sil:name>
			</sil:names>
		</special>
	</localeDisplayNames>
	<layout>
		<!--  Enter the orientation attributes  -->
		<orientation>
			<characterOrder>left-to-right</characterOrder>
		</orientation>
	</layout>
	<characters>
		<exemplarCharacters>[a b c d ɖ e ɛ f g {gb} ɣ h {hw} i j k {kp} l m n {ny} ŋ o ɔ p r s {sh} t u v w x {xw} y z ʒ]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[q]</exemplarCharacters>
		<exemplarCharacters type="index">[A B C D Ɖ E Ɛ F G {Gb} Ɣ H {Hw} I J K {Kp} L M N {Ny} Ŋ O Ɔ P R S {Sh} T U V W X {Xw} Y Z Ʒ]</exemplarCharacters>
	</characters>
	<special>
		<sil:external-resources>
			<sil:font name="Charis SIL" types="default" features="cv43=0 cv46=1">
				<sil:url>https://lff.api.languagetechnology.org/family/charissil</sil:url>
			</sil:font>
			<sil:font name="Noto Sans">
				<!--types="ui"-->
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSans/NotoSans-Regular.ttf</sil:url>
			</sil:font>
			<sil:font name="Noto Serif">
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSerif/NotoSerif-Regular.ttf</sil:url>
			</sil:font>
			<sil:sampletext type="udhr">
				<sil:url>http://efele.net/udhr/d/udhr_ajg.txt</sil:url>
			</sil:sampletext>
		</sil:external-resources>
	</special>
</ldml>
