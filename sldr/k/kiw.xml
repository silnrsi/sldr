<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="0.0.1"/>
		<!-- name.en(kiw)="Northeast Kiwai" -->
		<language type="kiw"/>
		<special>
			<sil:identity defaultRegion="PG" script="Latn"/>
		</special>
	</identity>
	<localeDisplayNames>
		<special>
			<sil:names>
				<sil:name xml:lang="en">Kiwai, Northeast</sil:name>
			</sil:names>
		</special>
	</localeDisplayNames>
	<characters>
		<exemplarCharacters>[a b d e g h i k m n o p r s t u v ꞌ]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[c f j l q w x y z]</exemplarCharacters>
		<exemplarCharacters type="index">[A B D E G H I K M O P R S T U Ꞌ]</exemplarCharacters>
		<exemplarCharacters type="punctuation">[_ \- , ; \: ! ? . ‘ ’ “ ” ( )]</exemplarCharacters>
	</characters>
	<delimiters>
		<special>
			<sil:matched-pairs>
				<sil:matched-pair open="(" close=")"/>
				<sil:matched-pair open="[" close="]"/>
				<sil:matched-pair open="{" close="}"/>
			</sil:matched-pairs>
		</special>
	</delimiters>
	<collations>
		<collation type="standard">
			<cr><![CDATA[
				&Ꞌ < ''
			]]></cr>
			<special>
				<sil:simple>a/A
				b/B
				d/D
				e/E
				g/G
				i/I
				k/K
				m/M
				n/N
				o/O
				p/P
				r/R
				s/S
				t/T
				u/U
				ꞌ/Ꞌ
				'</sil:simple>
			</special>
		</collation>
	</collations>
	<special>
		<sil:external-resources>
			<sil:font name="Charis" types="default">
				<sil:url>https://lff.api.languagetechnology.org/family/charis</sil:url>
			</sil:font>
			<sil:font name="Noto Sans">
				<!--types="ui"-->
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSans/NotoSans-Regular.ttf</sil:url>
			</sil:font>
			<sil:font name="Noto Serif">
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSerif/NotoSerif-Regular.ttf</sil:url>
			</sil:font>
		</sil:external-resources>
