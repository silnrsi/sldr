<?xml version='1.0' encoding='utf-8'?>
<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
  <identity>
  	<special xmlns:sil="urn://www.sil.org/ldml/0.1">
   		 <sil:identity defaultRegion="SN"/>  <!-- country code - 2 letters -->
   	 </special>
    <language type="cwt"/>   <!-- language code - should match file name, 2 or 3 letters -->
    <script type="Latn"/>    <!-- script code -->
  </identity>
  <characters>
  	<!-- use draft="proposed" for more-than-typically reliable data -->
  	<!-- only necessary to put a single references attribute on the main exemplar list. -->
    <exemplarCharacters type="" draft="tentative" references="L001">[ a {aa} b d e {ee} f g h i {ii} j k l m n ñ ŋ o {oo} r s t u {uu} w y ]</exemplarCharacters>
    <exemplarCharacters type="auxiliary" draft="tentative">[  ]</exemplarCharacters>
    <exemplarCharacters type="index" draft="tentative">[ A {AA} B D E {EE} F G H I {II} J K L M N Ñ Ŋ O {OO} R S T U {UU} W Y ]</exemplarCharacters>
    <exemplarCharacters type="punct" draft="tentative" references="L002">[\- ‐ – — , ; \: ! ? . … ’ " “ ” « » ( ) \[ \] § @ * / \&amp; # † ‡]</exemplarCharacters>
  </characters>
  <numberingSystem id="latn" type="numeric" digits="0123456789"/>
  <delimiters>
 		<quotationStart>“</quotationStart>     <!-- outer quotes -->
		<quotationEnd>”</quotationEnd>
		<alternateQuotationStart>‘</alternateQuotationStart>  <!-- inner quotes -->
		<alternateQuotationEnd>’</alternateQuotationEnd>
  </delimiters>
  <layout>
    <orientation characters="left-to-right" lines="top-to-bottom"/>  <!-- right-to-left -->
  </layout>
  <special>
    <sil:external-resources>
		<!--check if language uses font features; if so, use syntax from original full template and reference ldml_fontsAndFeatures.css -->
      <sil:font engines="ot" name="Charis SIL" features="cv43=0" references="L003">
      </sil:font>
      <sil:font engines="gr" name="Charis SIL" features="Engs=0">
      </sil:font>
    </sil:external-resources>
  </special>
  <collations>
    <collation type="standard" references="L001">  <!-- generally type will be 'standard' -->
			<sil:simple>a/A 
            aa 
            b/B 
            d/D 
            e/E 
            ee 
            f/F 
            g/G 
            h/H 
            i/I 
            ii 
            j/J
            k/K 
            l/L 
            m/M 
            n/N
            ñ/Ñ 
            ŋ/Ŋ 
            o/O 
            oo 
            r/R 
            s/S 
            t/T 
            u/U 
            uu 
            w/W 
            y/Y</sil:simple>
			</special>
    </collation>
  </collations>
  <references>
  	<reference type="L001"> REAP; Guide d'orthographe kwatay, 1995.
  	</reference>
    <reference type="L002"> copied from fr.xml 
    </reference>
    <reference type="L003" uri="https://software.sil.org/charis/wp-content/uploads/sites/14/2015/11/CharisSIL-features5.000.pdf"> CharisSIL-features pdf file 
    </reference>
  </references>
</ldml>
