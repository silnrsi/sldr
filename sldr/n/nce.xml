<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="0.0.1"/>
		<!-- name.en(nce)="Yale" -->
		<language type="nce"/>
		<special>
			<sil:identity defaultRegion="PG" script="Latn"/>
		</special>
	</identity>
	<localeDisplayNames>
		<languages>
			<language type="nce">Yalë</language>
		</languages>
		<special>
			<sil:names>
				<sil:name xml:lang="en">Yale</sil:name>
			</sil:names>
		</special>
	</localeDisplayNames>
	<characters>
		<exemplarCharacters>[a b d e {ë} f g h i j k m n o r s t u w y]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[c l p q v x z]</exemplarCharacters>
		<exemplarCharacters type="index">[A B D E Ë F G H I J K L M N O P R S T U V W Y]</exemplarCharacters>
		<exemplarCharacters type="punctuation">[\- – , ; \: ! ? . ‘ ’ “ ” ( )]</exemplarCharacters>
	</characters>
	<delimiters>
		<quotationStart>&lt;&lt;</quotationStart>
		<quotationEnd>&gt;&gt;</quotationEnd>
		<alternateQuotationStart>&lt;</alternateQuotationStart>
		<alternateQuotationEnd>&gt;</alternateQuotationEnd>
		<special>
			<sil:matched-pairs>
				<sil:matched-pair open="(" close=")"/>
				<sil:matched-pair open="[" close="]"/>
				<sil:matched-pair open="{" close="}"/>
			</sil:matched-pairs>
			<sil:quotation-marks>
				<sil:quotation open="&lt;&lt;" close="&gt;&gt;" level="3"/>
			</sil:quotation-marks>
		</special>
	</delimiters>
	<collations>
		<collation type="standard">
			<cr><![CDATA[
				&E < ë <<< Ë
			]]></cr>
			<special>
				<sil:simple>a/A
				b/B
				c/C
				d/D
				e/E
				ë/Ë
				f/F
				g/G
				h/H
				i/I
				j/J
				k/K
				l/L
				m/M
				n/N
				o/O
				p/P
				q/Q
				r/R
				s/S
				t/T
				u/U
				v/V
				w/W
				x/X
				y/Y
				z/Z</sil:simple>
			</special>
		</collation>
	</collations>
	<special>
		<sil:external-resources>
			<sil:font name="Charis SIL" types="default">
				<sil:url>https://lff.api.languagetechnology.org/family/charissil</sil:url>
			</sil:font>
			<sil:font name="Noto Sans">
				<!--types="ui"-->
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSans/NotoSans-Regular.ttf</sil:url>
			</sil:font>
			<sil:font name="Noto Serif">
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSerif/NotoSerif-Regular.ttf</sil:url>
			</sil:font>
		</sil:external-resources>
	</special>
</ldml>
