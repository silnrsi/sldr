<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="$Revision$"/>
		<language type="en"/>
		<territory type="MO"/>
		<special>
			<sil:identity source="cldr" defaultRegion="MO" script="Latn"/>
		</special>
	</identity>
	<dates>
		<timeZoneNames>
			<metazone type="Hong_Kong">
				<short>
					<generic>HKT</generic>
					<standard>HKT</standard>
					<daylight>HKST</daylight>
				</short>
			</metazone>
			<metazone type="Macau">
				<short>
					<generic>MST</generic>
					<standard>MST</standard>
					<daylight>MDT</daylight>
				</short>
			</metazone>
		</timeZoneNames>
	</dates>
	<numbers>
		<currencies>
			<currency type="MOP">
				<symbol>MOP$</symbol>
			</currency>
		</currencies>
	</numbers>
	<special>
		<sil:external-resources>
			<sil:font name="Charis" types="default">
				<sil:url>https://lff.api.languagetechnology.org/family/charis</sil:url>
			</sil:font>
			<sil:font name="Noto Sans">
				<!--types="ui"-->
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSans/NotoSans-Regular.ttf</sil:url>
			</sil:font>
			<sil:font name="Noto Serif">
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSerif/NotoSerif-Regular.ttf</sil:url>
			</sil:font>
		</sil:external-resources>
	</special>
</ldml>
