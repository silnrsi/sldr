<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<generation date="$Date$"/>
		<language type="tvu"/>
		<special>
			<sil:identity defaultRegion="CM"/>
		</special>
	</identity>
	<characters>
		<exemplarCharacters type="auxiliary">[d g j p q r v x z]</exemplarCharacters>
		<exemplarCharacters type="index">[A B C D E Ɛ F H I K L M {Mb} N {Nd} {Ng} {Nj} {Ny} Ŋ O Ɔ S T U W Y ]</exemplarCharacters>
		<exemplarCharacters type="punctuation">[, . ; ' " ! ?]</exemplarCharacters>
		<exemplarCharacters>[a á b c e é ɛ {ɛ́} f h i í k l m {mb} n {nd} {ng} {nj} {ny} ŋ o ó ɔ {ɔ́} s t u ú w y]</exemplarCharacters>
	</characters>
	<!--  Enter the plural rules (template example is for only one plural form)  -->
	<plurals>
		<pluralRule count="one">n is 1</pluralRule>
	</plurals>
</ldml>
