		<version number="0.0.1"/>
		<!-- name.en(kol)="Kol (Papua New Guinea)" -->
		<language type="kol"/>
		<special>
			<sil:identity defaultRegion="PG" script="Latn"/>
		</special>
	</identity>
	<localeDisplayNames>
		<special>
			<sil:names>
				<sil:name xml:lang="en">Kol</sil:name>
			</sil:names>
		</special>
	</localeDisplayNames>
	<characters>
		<exemplarCharacters>[a {aa} b e {ee} {ëë} g i {ii} k l m n {ng} o {oo} {ö} {öö} p r s t u {uu} w y]</exemplarCharacters>
