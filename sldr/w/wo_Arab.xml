<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="0.0.1"/>
		<!-- name.en(wol)="Wolof" -->
		<language type="wo"/>
		<script type="Arab"/>
		<special>
			<sil:identity defaultRegion="SN" script="Arab"/>
		</special>
	</identity>
	<localeDisplayNames>
		<languages>
			<language type="wo_Arab">Wolof</language>
		</languages>
		<special>
			<sil:names>
				<sil:name xml:lang="en">Wolof</sil:name>
			</sil:names>
		</special>
	</localeDisplayNames>
	<layout>
		<!--  Enter the orientation attributes  -->
		<orientation>
			<characterOrder>right-to-left</characterOrder>
		</orientation>
	</layout>
	<characters>
		<exemplarCharacters>[\u064E \u08F5 \u0650 \u08F7 \u0657 \u064F \u08F9 \u08FA \u08F4 ا ب ݒ ت ݖ ج خ د ر س ݝ ف ق ك ک گ ل م ن ݧ و ي {مب} {مݒ} {ند} {نگ} {نج} {نت} {نݖ} {نك} {نک} {نق}]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[\u200C\u200D\u200E\u200F]</exemplarCharacters>
	</characters>
	<special>
		<sil:external-resources>
			<sil:font name="Harmattan" types="default" features="cv12=1 cv70=2 cv74=1">
				<sil:url>https://wirl.api.sil.org/Harmattan</sil:url>
			</sil:font>
			<sil:font name="Scheherazade New" types="default" features="cv12=1 cv62=1 cv70=2 cv74=1">
				<sil:url>https://wirl.api.sil.org/Scheherazade</sil:url>
			</sil:font>
		</sil:external-resources>
	</special>
</ldml>
