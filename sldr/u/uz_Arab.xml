							<day type="mon" draft="contributed">د.</day>
							<day type="tue" draft="contributed">س.</day>
							<day type="wed" draft="contributed">چ.</day>
							<day type="thu" draft="contributed">پ.</day>
							<day type="fri" draft="contributed">ج.</day>
