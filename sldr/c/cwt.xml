<?xml version="1.0" encoding="utf-8"?>
<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="0.0.1"/>
		<language type="cwt"/>
		<special>
			<sil:identity defaultRegion="SN" script="Latn"/>
		</special>
	</identity>
	<layout>
		<orientation>
			<characterOrder>left-to-right</characterOrder>
			<lineOrder>top-to-bottom</lineOrder>
		</orientation>
	</layout>
	<characters>
		<exemplarCharacters draft="tentative" references="L001">[a {aa} b d e {ee} f g h i {ii} j k l m n ñ ŋ o {oo} r s t u {uu} w y]</exemplarCharacters>
		<exemplarCharacters type="punctuation" draft="tentative" references="L002">[\- ‐ – — , ; \: ! ? . … ’ " “ ” « » ( ) \[ \] § @ * / \&amp; # † ‡]</exemplarCharacters>
		<exemplarCharacters type="index" draft="tentative">[A {AA} B D E {EE} F G H I {II} J K L M N Ñ Ŋ O {OO} R S T U {UU} W Y]</exemplarCharacters>
	</characters>
	<delimiters>
		<quotationStart>“</quotationStart>
		<quotationEnd>”</quotationEnd>
		<alternateQuotationStart>‘</alternateQuotationStart>
		<alternateQuotationEnd>’</alternateQuotationEnd>
	</delimiters>
	<numbers>
		<defaultNumberingSystem>latn</defaultNumberingSystem>
	</numbers>
	<collations>
		<collation type="standard" references="L001">
			<cr><![CDATA[
				&A < aa <<< Aa <<< AA
				&E < ee <<< Ee <<< EE
				&I < ii <<< Ii <<< II
				&N < ñ <<< Ñ
				&O < oo <<< Oo <<< OO
				&U < uu <<< Uu <<< UU
			]]></cr>
			<special>
			<sil:simple>a/A
			aa
			b/B
			d/D
			e/E
			ee
			f/F
			g/G
			h/H
			i/I
			ii
			j/J
			k/K
			l/L
			m/M
			n/N
			ñ/Ñ
			ŋ/Ŋ
			o/O
			oo
			r/R
			s/S
			t/T
			u/U
			uu
			w/W
			y/Y</sil:simple>
			</special>
		</collation>
	</collations>
	<references>
		<reference type="L001">REAP; Guide d'orthographe kwatay, 1995.</reference>
		<reference type="L002">copied from fr.xml</reference>
		<reference type="L003" uri="https://software.sil.org/charis/wp-content/uploads/sites/14/2015/11/CharisSIL-features5.000.pdf">CharisSIL-features pdf file</reference>
	</references>
	<special>
		<sil:external-resources>
			<sil:font name="Charis SIL" features="cv43=0" engines="ot" references="L003"/>
		</sil:external-resources>
	</special>
</ldml>
