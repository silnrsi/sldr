<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="0.0.1"/>
		<!-- name.en(snk)="Soninke" -->
		<language type="snk"/>
		<script type="Arab"/>
		<special>
			<sil:identity defaultRegion="ML" script="Arab"/>
		</special>
	</identity>
	<localeDisplayNames>
		<languages>
			<language type="snk">Sooninkanxanne</language>
		</languages>
		<special>
			<sil:names>
				<sil:name xml:lang="en">Soninke</sil:name>
			</sil:names>
		</special>
	</localeDisplayNames>
	<layout>
		<!--  Enter the orientation attributes  -->
		<orientation>
			<characterOrder>right-to-left</characterOrder>
		</orientation>
	</layout>
	<characters>
		<exemplarCharacters>[\u064E \u064F \u0650 ا ب ݒ ت ݖ ج خ د ر س ݝ ف ق ک گ ل م ن ݧ ہ و ي]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[\u200C\u200D\u200E\u200F]</exemplarCharacters>
	</characters>
	<special>
		<sil:external-resources>
			<sil:font name="Harmattan" types="default">
				<sil:url>https://lff.api.languagetechnology.org/family/harmattan</sil:url>
			</sil:font>
			<sil:font name="Noto Sans Arabic">
				<!--types="ui"-->
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSansArabic/NotoSansArabic-Regular.ttf</sil:url>
			</sil:font>
			<sil:font name="Scheherazade New" types="default" features="cv62=1">
				<sil:url>https://lff.api.languagetechnology.org/family/scheherazadenew</sil:url>
			</sil:font>
		</sil:external-resources>
	</special>
</ldml>
