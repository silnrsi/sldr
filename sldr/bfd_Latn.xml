<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<generation date="$Date$"/>
		<language type="bfd"/>
		<special>
			<sil:identity defaultRegion="CM"/>
		</special>
	</identity>
	<characters>
		<exemplarCharacters type="auxiliary">[r]</exemplarCharacters>
		<exemplarCharacters type="index">[A B C D E Ɛ Ə F G H I J K ̍l M N Ŋ O Ɔ P R S T U V W Y Z]</exemplarCharacters>
		<exemplarCharacters type="punctuation">[, . : ; ' " ? !]</exemplarCharacters>
		<exemplarCharacters>[a á ā â ǎ b c d e é ē ê ě ɛ {ɛ́} {ɛ̄} {ɛ̂} {ɛ̌} ə {ə́} {ə̄} {ə̂} {ə̌} f g h i í ī î ǐ j ̍k ʼ l m n ŋ o ó ō ô ǒ ɔ {ɔ́} {ɔ̄} {ɔ̂} {ɔ̌} p s t u ú ū û ǔ v w y]</exemplarCharacters>
	</characters>
	<!--  Enter the plural rules (template example is for only one plural form)  -->
	<plurals>
		<pluralRule count="one">n is 1</pluralRule>
	</plurals>
</ldml>
