<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="0.0.1"/>
		<!-- name.en(ky)="Kyrgyz" -->
		<language type="ky"/>
		<territory type="CN"/>
		<special>
			<sil:identity defaultRegion="CN" script="Arab"/>
		</special>
	</identity>
	<localeDisplayNames>
		<languages>
			<language type="ky_CN">кыргызча</language>
		</languages>
		<special>
			<sil:names>
				<sil:name xml:lang="en">Kyrgyz</sil:name>
			</sil:names>
		</special>
	</localeDisplayNames>
	<layout>
		<!--  Enter the orientation attributes  -->
		<orientation>
			<characterOrder>right-to-left</characterOrder>
		</orientation>
	</layout>
	<characters>
		<exemplarCharacters>[ئ ا ب ت ج ح خ د ر ز س ش ع ق ك ل م ن و ى ي پ چ ڧ ڭ گ ۅ ۇ ۉ ۋ ە]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[\u200C\u200D\u200E\u200F]</exemplarCharacters>
	</characters>
	<special>
		<sil:external-resources>
			<sil:font name="Noto Sans Arabic">
				<!--types="ui"-->
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSansArabic/NotoSansArabic-Regular.ttf</sil:url>
			</sil:font>
			<sil:font name="Scheherazade New" types="default" features="cv54=1">
				<sil:url>https://wirl.api.sil.org/Scheherazade</sil:url>
			</sil:font>
		</sil:external-resources>
	</special>
</ldml>
