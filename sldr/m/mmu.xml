	<identity>
		<version number="0.0.1"/>
		<generation date="$Date$"/>
		<!-- name.en(mmu)="Mmaala" -->
		<language type="mmu"/>
		<special>
			<sil:identity defaultRegion="CM" script="Latn"/>
		</special>
	</identity>
	<localeDisplayNames>
		<languages>
			<language type="mmu">Numála</language>
		</languages>
		<special>
			<sil:names>
				<sil:name xml:lang="en">Mmaala</sil:name>
			</sil:names>
		</special>
	</localeDisplayNames>
	<characters>
		<exemplarCharacters>[a á b c d e é ɛ {ɛ́} f g h i í k l m n {ny} ŋ o ó ɔ {ɔ́} p s t u ú w y]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[j q r v x z]</exemplarCharacters>
		<exemplarCharacters type="index">[A B C D E Ɛ F G H I K L M N {NY} Ŋ O Ɔ P S T U W Y]</exemplarCharacters>
		<exemplarCharacters type="punctuation">[, . ; ' "]</exemplarCharacters>
	</characters>
	<!--  Enter the plural rules (template example is for only one plural form)  -->
