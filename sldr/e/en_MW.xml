<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="$Revision$"/>
		<language type="en"/>
		<territory type="MW"/>
		<special>
			<sil:identity source="cldr" defaultRegion="MW" script="Latn"/>
		</special>
	</identity>
	<dates>
		<timeZoneNames>
			<metazone type="Africa_Central">
				<short>
					<standard>CAT</standard>
				</short>
			</metazone>
			<metazone type="Africa_Eastern">
				<short>
					<standard>EAT</standard>
				</short>
			</metazone>
			<metazone type="Africa_Southern">
				<short>
					<standard>SAST</standard>
				</short>
			</metazone>
			<metazone type="Africa_Western">
				<short>
					<generic>WAT</generic>
					<standard>WAT</standard>
					<daylight>WAST</daylight>
				</short>
			</metazone>
		</timeZoneNames>
	</dates>
	<numbers>
		<currencies>
			<currency type="MWK">
				<symbol>MK</symbol>
			</currency>
		</currencies>
	</numbers>
	<special>
		<sil:external-resources>
			<sil:font name="Charis" types="default">
				<sil:url>https://lff.api.languagetechnology.org/family/charis</sil:url>
			</sil:font>
			<sil:font name="Noto Sans">
