<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="0.0.1"/>
		<!-- name.en(biq)="Bipi" -->
		<language type="biq"/>
		<special>
			<sil:identity defaultRegion="PG" usage="unused" script="Latn"/>
		</special>
	</identity>
	<localeDisplayNames>
		<special>
			<sil:names>
				<sil:name xml:lang="en">Bipi</sil:name>
			</sil:names>
		</special>
	</localeDisplayNames>
	<characters>
		<exemplarCharacters>[a b {ch} e h i {ii} j k l m {mw} n {ndr} {ng} {ny} o p {pw} r s t {tj} u w y]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[c d f g q v x z]</exemplarCharacters>
	</characters>
	<special>
		<sil:external-resources>
			<sil:font name="Charis" types="default">
				<sil:url>https://lff.api.languagetechnology.org/family/charis</sil:url>
			</sil:font>
			<sil:font name="Noto Sans">
				<!--types="ui"-->
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSans/NotoSans-Regular.ttf</sil:url>
			</sil:font>
			<sil:font name="Noto Serif">
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSerif/NotoSerif-Regular.ttf</sil:url>
			</sil:font>
		</sil:external-resources>
	</special>
</ldml>
