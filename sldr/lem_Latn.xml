<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<generation date="$Date$"/>
		<!-- name.en(lem)="Nomaande" -->
		<language type="lem"/>
		<special>
			<sil:identity defaultRegion="CM"/>
		</special>
	</identity>
	<characters>
		<exemplarCharacters type="auxiliary" draft="unconfirmed">[n p q r v x z]</exemplarCharacters>
		<exemplarCharacters type="index" draft="unconfirmed">[A B C D E Ɛ F G H I J K L M {NY} Ŋ O Ɔ S T U V W Y ʼ ]</exemplarCharacters>
		<exemplarCharacters type="punctuation" draft="unconfirmed">[, . ; ' "]</exemplarCharacters>
		<exemplarCharacters draft="unconfirmed">[a á b c d e é ɛ {ɛ́} f g h i í j k l m {ny} ŋ o ó ɔ {ɔ́} s t u ú w y ʼ]</exemplarCharacters>
	</characters>
	<!--  Enter the plural rules (template example is for only one plural form)  -->
	<plurals>
		<pluralRule count="one">n is 1</pluralRule>
	</plurals>
</ldml>
