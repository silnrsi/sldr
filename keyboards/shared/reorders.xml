<keyboard>
    <transforms type="reorder">
        <!-- When mapping from USE, use the following values as a basis:
            O B IND N GB    0
            SUB H HN        5
            MPre            10
            MAbv SMAbv      15
            MBlw SMBlw      20
            MPst            25
            VPre            30
            VAbv            35
            VBlw            40
            VPst            45
            VMPre           50
            VMAbv           55
            VMBlw           60
            VMPst           65
            FAbv            70
            FBlw            75
            FPst            80
            FM              85
                But bear in mind the mapping isn't algorithmic
        -->
        <!-- Myanmar based on UTN#11 -->
        <reorder from="[\u1000-\u102C\u103F-\u1049\u104E\u1050-\u1057\u105A-\u105D\u1061-\u1065\u106E-\u1070\u1075-\u1081\u1083\u108E\u109F\uA9E0-\uA9E4\uA9E7-\uA9EF\uA9FA-\uA9FF\uAA60-\uAA6F\uAA71-\uAA76\uAA7A\uAA7E\uAA7F]" order="0"/>
        <reorder from="[\uFE00-\uFE0F]" tertiary="1"/>
        <reorder from="\u1039[\u1000-\u101C\u101E\u1020\u1021\u1050\u1051\u105A-\u105D]" order="5" tertiary_base="1"/>
        <reorder from="\u103A" order="10"/>
        <reorder from="[\u103B\u105E\u105F]" order="15"/>
        <reorder from="\u103C" order="20"/>
        <reorder from="[\u103D\u1082]" order="25"/>
        <reorder from="[\u103E\u1082]\u103A" order="27"/>      <!-- Mon asat -->
        <reorder from="[\u103E\u1060]" order="27"/>
        <reorder from="[\u1031\u1084]" order="30"/>
        <reorder from="[\u102D\u102E\u1033-\u1035\u1071-\u1074\u1085\u109D\uA9E5]" order="35"/>
        <reorder from="[\u102F\u1030\u1048\u1059]" order="40"/>
        <reorder from="\u1086" order="54"/>
        <reorder from="[\u1032\u1036]" order="55"/>
        <reorder from="\u1037" order="60"/>
        <reorder from="[\u1038\u1087-\u108D\u108F\u109A-\u109C\uAA7B-\uAA7D]" order="65"/>
        <reorder from="[\uA9E6\uAA70]" order="70"/>

        <!-- Khmer Unicode spec based with extras from Didi -->
        <reorder from="[\u1780-\u17B3\u17D2\u17DB\u17E0-\u17E9]" order="0"/>
        <reorder from="[\u200C\u200D][\u17C9\u17CA]" tertiary="5"/>
        <transfrom from="[\u17C9\u17CA]" tertiary="5"/>
        <reorder from="\u17CC" order="5"/>
        <reorder from="\u17D2[\u1780-\u17A2]" order="5"/>
        <reorder from="[\u200C\u200D]" order="27"/>
        <reorder from="[\u17BE-\u17C5]" order="30"/>
        <reorder from="[\u17B7-\u17BA\u17DD]" order="35"/>
        <reorder from="[\u17BB-\u17BD]" order="40"/>
        <reorder from="\u17B6" order="0"/>
        <reorder from="[\u17C6\1u7CB\u17CD-\u17D1\u17D3]" order="50"/>
        <reorder from="[\u17C7\u17C8]" order="85"/>

        <!-- Devanagari -->
        <!-- repha is keyboard specific and not part of canonical ordering -->
        <reorder from="\u0930\u094D\uFDD0" order="-1"/>       <!-- magic mark repha, remove in final -->
        <reorder from="[\u0904-\u0939\u093D\u0958-\u0961\u0964-\u097F\u0950\u0953\u0954]" order="0"/>
        <reorder from="\u093C" tertiary="3"/>
        <reorder from="\u094D[\u200C\u200D\u0915-0939]" order="5" tertiary_base="1"/>
        <reorder from="\u094D[\u200C\u200D][\u0915-0939]" order="5" tertiary_base="1"/>
        <reorder from="\u094D" order="5" tertiary_base="1"/>
        <reorder from="\u094E" order="30"/>
        <reorder from="[\u093A\u0945-\u0948\u0955]" order="35"/>
        <reorder from="[\u0941-\u0944\u0952\u0962\u0963]" order="40"/>
        <reorder from="[\u093B\u093E\u0940\u0949-\u094C\u094F]" order="45"/>
        <reorder from="[\u0900-\u0902\u0951]" order="55"/>
        <reorder from="\u0952" order="60"/>
        <reorder from="\u0903" order="65"/>

        <!-- Tai Tham -->
        <reorder from="[\u1A20-\u1A57\u1A61\u1A63\u1A64\u1A80-\u1A89\u1A90-\u1A99]" order="0"/>
        <reorder from="\u1A60" order="127"/>      <!-- max possible order -->
        <reorder from="[\u1A6E-\u1A72]" order="30"/>
        <reorder from="[\u1A69\u1A6A\u1A6C]" order="40"/>
        <reorder from="[\u1A62\u1A65-\u1A68\u1A6B\u1A73]" order="42"/>
        <reorder from="[\u1A74]" order="45"/>
        <reorder from="[\u1A75-\u1A7C]" order="55"/>
        <reorder from="[\u1A7F]" order="57"/>
        <reorder from="\u1A60\u1A45" before="\u1A6B" order="10"/>
        <reorder from="\u1A60\u1A45" before="[\u1A20-\u1A57\u1A6B][\u1A75-\u1A7C]" order="10"/>
        <reorder from="\u1A60[\u1A75-\u1A7C]\u1A45" before="[\u1A20-\u1A57\u1A6B]" order="10 55 10"/>
    </transforms>

    <transforms type="final">
        <transform from="\u103A\u1037" to="\u1037\u103A"/>
        <transform from="\u0930\u094D\uFDD0" to="\u0930\u094D"/>    <!-- strip repha marker. Usually in keyboard -->
        <transform from="\u103A\u1037\u1037" error="1"/>
        <transform from="[\u102D\u102E\u1033-\u1035\u1071-\u1074\u1085\u109D\uA9E5][\u102D\u102E\u1033-\u1035\u1071-\u1074\u1085\u109D\uA9E5]" error="1"/>
        <transform from="[\u102F\u1030\u1048\u1059][\u102F\u1030\u1048\u1059]" error="1"/>
        <transform from="\u1031\u1031\u1031" error="1"/>
        <transform from="\u1032\u1032" error="1"/>
        <transform from="\u1036\u1036" error="1"/>
        <transform from="\u1037\u1037" error="1"/>
        <transform from="\u1039\u1039" error="1"/>
        <transform from="\u103A\u103A" error="1"/>
        <transform from="\u103B\u103B" error="1"/>
        <transform from="\u103C\u103C" error="1"/>
        <transform from="\u103D\u103D" error="1"/>
        <transform from="\u103E\u103E" error="1"/>

        <!-- Lots of cleaning up for Khmer -->
        <transform from="\u17C1\u17B8" to="\u17BE"/>
        <transform from="\u17C1\u17B6" to="\u17C4"/>
    </transforms>
</keyboard>
