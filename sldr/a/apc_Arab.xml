<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="0.0.1"/>
		<!-- name.en(apc)="North Levantine Arabic" -->
		<language type="apc"/>
		<script type="Arab"/>
		<special>
			<sil:identity defaultRegion="SY" script="Arab"/>
		</special>
	</identity>
	<localeDisplayNames>
		<languages>
			<language type="apc-Arab">شامي</language>
		</languages>
		<special>
			<sil:names>
				<sil:name xml:lang="en">Arabic, North Levantine Spoken</sil:name>
			</sil:names>
		</special>
	</localeDisplayNames>
	<layout>
		<!--  Enter the orientation attributes  -->
		<orientation>
			<characterOrder>right-to-left</characterOrder>
		</orientation>
	</layout>
	<characters>
		<exemplarCharacters>[\u064B \u064E \u064F \u0650 \u0651 ء آ أ ؤ إ ئ ا ب ة ت ث ج ح خ د ذ ر ز س ش ص ض ط ظ ع غ ف ق ك ل م ن ه و ى ي ]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[\u200C\u200D\u200E\u200F]</exemplarCharacters>
	</characters>
	<special>
		<sil:external-resources>
			<sil:font name="Noto Sans Arabic" types="ui">
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSansArabic/NotoSansArabic-Regular.ttf</sil:url>
			</sil:font>
			<sil:font name="Scheherazade">
				<sil:url>https://wirl.api.sil.org/Scheherazade&amp;type=ttf</sil:url>
			</sil:font>
		</sil:external-resources>
	</special>
</ldml>
